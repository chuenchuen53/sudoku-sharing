    "prettier": "2.7.1",
    "stylelint": "14.14.0",
    "stylelint-config-recommended-scss": "7.0.0",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard-scss": "5.0.0",
    "typescript": "~4.7.4",
    "unplugin-auto-import": "0.11.4",
    "unplugin-vue-components": "0.22.9",
    "vite": "3.0.9",
    "vitest": "0.23.0",
    "vue-tsc": "0.40.7"
