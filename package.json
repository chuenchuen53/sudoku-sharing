    "lodash.combinations": "18.11.1",
    "pinia": "2.0.21",
    "sass": "1.55.0",
    "vue": "3.2.38",
    "vue-router": "4.1.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.1.4",
    "@types/jsdom": "20.0.0",
    "@types/lodash": "4.14.186",
    "@types/node": "16.11.56",
    "@vitejs/plugin-vue": "3.0.3",
    "@vitest/coverage-c8": "0.24.5",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "11.0.0",
